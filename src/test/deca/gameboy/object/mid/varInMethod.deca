class A {
    int x = 2;

    void calc() {
        int y = 2 * x; // 2 * 2 = 4
        int z = 8 * x; // 8 * 2 = 16
        int x = x + y + z + 42; // 2 + 4 + 16 + 42 = 64
        int a = 2 * x; // 2 * 64 = 128
        if (this.x != 2) {
            println();
        }
        if (y != 4) {
            println();
        }
        if (z != 16) {
            println();
        }
        if (x != 64) {
            println();
        }
        if (a != 128) {
            println();
        }
    }
}

{
    A a = new A();
    a.calc();

    if (a.x != 2) {
        println();
    }
}
