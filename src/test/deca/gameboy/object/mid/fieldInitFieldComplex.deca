// Description :
//    Test d'une classe où l'initilisation d'un attribut utilise
//    un autre attribut initialisé après
//
// Resultats :
//    0 0 20

class A {
    int a = this.getC();
    int b = c;
    int c = 20;

    int getC() {
        return this.c;
    }

    void check() {
        if (a != 0) {
            println();
        }
        if (b != 0) {
            println();
        }
        if (c != 20) {
            println();
        }
    }
}

{
    A a = new A();
    a.check();

    if (a.a != 0) {
        println();
    }
    if (a.b != 0) {
        println();
    }
    if (a.c != 20) {
        println();
    }
}
